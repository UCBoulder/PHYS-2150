  },
  "eqe": {
    "defaults": {
      "start_wavelength": 350.0,
      "end_wavelength": 720.0,
      "step_size": 10.0
    },
    "validation": {
      "cell_number": "^\\d{3}$",
      "pixel_range": [1, 8]
    },
    "stability": {
      "default_wavelength": 550,
      "default_duration_min": 5,
      "duration_range": [1, 60],
      "default_interval_sec": 2,
      "interval_range": [1, 60]
